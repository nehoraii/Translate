<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="org.example.entity.EditorEntity">
        <table name="Editor" schema="public" catalog="Translate"/>
        <attributes>
            <id name="id">
                <column name="ID" precision="-1"/>
            </id>
            <basic name="name">
                <column name="Name"/>
            </basic>
            <basic name="phone">
                <column name="Phone"/>
            </basic>
            <basic name="email">
                <column name="Email" nullable="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
